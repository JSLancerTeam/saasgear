{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/react-hooks": "^4.0.0",
    "@craco/craco": "^5.8.0",
    "@headlessui/react": "^0.2.0",
    "@hookform/resolvers": "^1.0.0",
    "@reduxjs/toolkit": "^1.4.0",
    "@sentry/react": "^5.27.6",
    "@sentry/tracing": "^5.27.6",
    "@stripe/react-stripe-js": "^1.1.2",
    "@stripe/stripe-js": "^1.11.0",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link": "^1.2.14",
    "apollo-link-context": "^1.0.20",
    "apollo-link-error": "^1.1.13",
    "apollo-upload-client": "^14.1.2",
    "classnames": "^2.2.6",
    "dayjs": "^1.9.4",
    "dompurify": "^2.2.3",
    "draft-js": "^0.11.7",
    "draftjs-to-html": "^0.9.1",
    "fb": "^2.0.0",
    "graphql": "^15.4.0",
    "graphql.macro": "^1.4.2",
    "html-to-draftjs": "^1.5.0",
    "lodash": "^4.17.20",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-apollo-network-status": "^5.0.1",
    "react-dom": "^17.0.1",
    "react-draft-wysiwyg": "^1.14.5",
    "react-hook-form": "^6.10.1",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.0",
    "react-toastify": "^6.1.0",
    "redux": "^4.0.5",
    "styled-components": "^5.2.1",
    "yup": "^0.29.3"
  },
  "scripts": {
    "build:css": "postcss src/assets/css/tailwind.css -o src/assets/css/main.css",
    "watch:css": "postcss src/assets/css/tailwind.css -o src/assets/css/main.css",
    "lint": "stylelint **/*.{js,jsx} && eslint --ext .js --ext .jsx .",
    "lint:fix": "eslint --ext .js --ext .jsx . --fix",
    "lint:staged": "lint-staged",
    "lint:prod": "NODE_ENV=production npm run lint",
    "start": "npm run watch:css && craco start",
    "build": "npm run lint:prod && npm run build:css && craco build",
    "eject": "craco eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.js": [
      "npm run lint:fix",
      "git add --force"
    ],
    "*.json": [
      "prettier --write",
      "git add --force"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run lint"
    }
  },
  "devDependencies": {
    "@tailwindcss/ui": "^0.7.2",
    "autoprefixer": "^9.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.12.1",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-lodash": "^7.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.1",
    "postcss": "^8.2.1",
    "postcss-cli": "^8.3.1",
    "pre-commit": "^1.2.2",
    "prettier": "^2.1.2",
    "stylelint": "^13.7.2",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "tailwindcss": "^1.9.6"
  }
}
